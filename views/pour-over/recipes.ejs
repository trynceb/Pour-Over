<!<%- include('../partials/header') %>

<h1>This page will show you a list of all your recipes.</h1>

<table id="list">
    <thead>
        <tr>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        <% recipes.forEach(function(r) { %>
            <tr>
                <td>
                    <%= r.name %>
                </td>
                <td><a href="/movies/<%= r._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>